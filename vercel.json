{
  "version": 2,
  "builds": [
    {
      "src": "bot.py",
      "use": "@vercel/python"
    },
     {
      "src": "route.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/bot.py"
    },
    {
      "src": "/",
      "dest": "/bot.py"
    },
    {
      "src": "/",
      "dest": "/route.py"
    },
  ]
}
